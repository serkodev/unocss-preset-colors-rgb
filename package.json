  "description": "",
  "keywords": [],
  "homepage": "https://github.com/serkodev/unocss-preset-colors-rgb#readme",
  "bugs": {
    "url": "https://github.com/serkodev/unocss-preset-colors-rgb/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serkodev/unocss-preset-colors-rgb.git"
  },
  "main": "./dist/index.global.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "tsx ./src/index.ts",
    "dev": "tsx watch ./src/index.ts",
    "prepublishOnly": "pnpm build",
    "play": "pnpm -F ./playground dev",
    "release": "pnpm lint && bump --commit --push --tag && npm publish"
  },
  "packageManager": "pnpm@8.4.0",
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.6",
    "@types/node": "^22.4.1",
    "@unocss/core": "^0.62.2",
    "eslint": "^8.40.0",
    "tsup": "^8.2.4",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "version-bump-prompt": "^6.1.0"
  },
  "peerDependencies": {
    "@unocss/core": ">=0.51.12"
  }
}
